// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Wine {
  id          Int      @id @default(autoincrement())
  name        String
  year        Int
  type        String
  varietal    String
  rating      Float?
  consumed    Boolean?
  dateConsumed DateTime?
}